<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
 PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
 "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="gw_member">
	<!-- Support -->
	<!-- 임직원정보 리스트 -->
	<select id="employee_list" resultType="com.hanbit.vo.VO_employees">
		select * from gw_member order by id
	</select>
	<!-- employees search -->
	<select id="employee_search" parameterType="Map" resultType="com.hanbit.vo.VO_employees">
		select * from gw_member where ${type}=#{content} order by id
	</select>
	
	<select id="getEmailList" resultType="com.hanbit.vo.VO_email">
		select * from gw_email order by regdate desc
	</select>
	<insert id="writeEmail" parameterType="com.hanbit.vo.VO_email">
		insert into gw_email values('kyk@gmail.com', #{email_addr}, #{subject}, #{content}, #{email_file}, sysdate)
	</insert>
	<select id="boardList" resultType="com.hanbit.vo.VO_board">
		select * from gw_board	
	</select>
</mapper>
